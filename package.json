{
  "name": "mpshell",
  "displayName": "mpShell",
  "description": "Python REPL shell",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mpshell.selectport",
    "onCommand:mpshell.syncdata",
    "onView:fileList"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "fileList",
          "name": "Lista de Archivos"
        }
      ]
    },
    "commands": [
      {
        "command": "fileList.obtener",
        "title": "Obtener"
      },
      {
        "command": "fileList.eliminar",
        "title": "Eliminar"
      },
      {
        "command": "mpshell.selectport",
        "title": "Select Port"
      },
      {
        "command": "mpshell.soft_reset",
        "title": "Soft Reset"
      },
      {
        "command": "mpshell.syncdata",
        "title": "Sync Files"
      },
      {
        "command": "mpshell.changesetting",
        "title": "Modificar configuraci√≥n"
      },
      {
        "command": "mpshell.sendcurrentfile",
        "title": "Send current file"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "fileList.obtener",
          "group": "inline",
          "when": "view == fileList && viewItem == archivo"
        },
        {
          "command": "fileList.eliminar",
          "group": "inline",
          "when": "view == fileList && viewItem == archivo"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "copy \"%CD%\\src\\command\\*.*\" \"%CD%\\out\\command\\\" && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.65.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  },
  "dependencies": {
    "serialport": "^10.5.0"
  }
}